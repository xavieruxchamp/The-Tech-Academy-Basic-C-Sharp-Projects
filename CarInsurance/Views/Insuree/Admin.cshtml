@model List<CarInsurance.Controllers.QuoteViewModel>

@{
    ViewBag.Title = "Admin";
}

<h2>Admin</h2>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email Address</th>
            <th>Quote</th>
        </tr>
    </thead>
    @foreach (var quote in Model)
    {
        <tr>
            <td>@quote.FirstName</td>
            <td>@quote.LastName</td>
            <td>@quote.EmailAddress</td>
            <td>@quote.Quote</td>
        </tr>
    }
</table>
